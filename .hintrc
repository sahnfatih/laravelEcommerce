{
  "extends": [
    "development"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-overflow-scrolling",
          "-webkit-tap-highlight-color",
          "text-decoration-skip-ink",
          "text-align: -webkit-match-parent"
        ]
      }
    ],
    "css-prefix-order": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 125",
    "not chrome <= 126",
    "not edge <= 125",
    "not firefox <= 127",
    "not safari <= 17.5",
    "not samsung <= 25"
  ]
}